{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "installCommand": "npm i",
    "outputDirectory": "build/",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "api/index.ts"
        }
    ],
    "functions": {
        "api/**/*.ts": {
            "includeFiles": "public/**/*"
        }
    }
}
